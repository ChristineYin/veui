{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "repositories": ["ecomfe/veui"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    },
    {
      "matchPackageNames": ["dls-icons-vue", "dls-illustrations-vue", "dls-graphics"],
      "matchBaseBranches": ["main"],
      "enabled": true
    }
  ],
  "rangeStrategy": "bump",
  "commitBody": "[skip ci]"
}
